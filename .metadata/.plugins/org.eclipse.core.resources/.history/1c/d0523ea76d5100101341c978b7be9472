
public class Loop2 {

	public static void main(String[] args) {
		
		int num1 = 0;
		int num2 = 0;
		
		for(num1=2; num1<=9;num1++) {
			for(num2=2; num2<=9; num2++) {
				System.out.printf(num2 +"X"+ num1 + "=" + num1 * num2 + "\t");
				if(num2 == 9) {
					System.out.println(num2 +"X"+ num1 + "=" + num1 * num2);
				}
				
			}
		}

	}

}
