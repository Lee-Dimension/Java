package NumberBaseBall;

public class NumberBaseBall21 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] allCombi = new int[720];
		
		for(int i=0; i<720; i++) {
			for(int j=0; j<10; j++) {
				for(int k=0; k<10; k++) {
					for(int l=0; l<10; l++) {
						if(j!=k &&j!=l &&k!=l) {
						allCombi[i] = j*100+k*10+l;
						}
					}
				}
			}
		}
		System.out.println(allCombi[300]);
		
	}

}
