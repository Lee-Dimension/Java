package BaekJoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int [] result = new int[500];
		int num1, num2;
	
		
		while(true) {
			int i = 0;
			num1 = sc.nextInt();
			num2 = sc.nextInt();
			result[i] = num1+num2;
			if(result[i]==0) {
				break;
			}
			i++;
		}
		for(int i = 0; i<result.length;i++) {
			System.out.println(result[i]);
		}	
	}
}
