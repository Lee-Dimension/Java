
public class Loop {

	public static void main(String[] args) {
	
		int foldCount= 0;
		int squareCount = 0;
		for(int i=0, s=1; s<10000; i++, s*=2) {
		    if(s >= 500) {
		        a = i;
		        b = s;
		        break;
		    }
		}
			System.out.println("접은 횟수 : " + a);
			System.out.println("사각형 개수 : " + b);
	}

}
