package BaekJoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int arr[];
		int num1,num2;
		int inputCount;
		
		inputCount = sc.nextInt();
		arr = new int[inputCount];
		for(int i=0; i <inputCount;i++) {
			num1 = sc.nextInt();
			num2 = sc.nextInt();
			arr[i] = num1+num2;
		}
		
		for(int i=0; i<inputCount; i++) {
			System.out.println("Case #"+(i+1)+" "+arr[i]);
		}
		
	}
}
