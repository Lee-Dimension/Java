package BaekJoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int [] result = new int[10];
		int num1, num2;
		int i = 0;
		
		while(true) {
			num1 = sc.nextInt();
			num2 = sc.nextInt();
			result[i] = num1+num2;
			if(result[i]==0) {
				break;
			}
			i++;
		}
		for(int j = 0; j<result.length;j++) {
			System.out.println(result[j]);
		}	
	}
}
