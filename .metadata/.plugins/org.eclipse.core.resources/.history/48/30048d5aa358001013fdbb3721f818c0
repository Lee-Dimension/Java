package BaekJoon;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int arr[];
		int num1[];
		int num2[];
		int inputCount;
		
		inputCount = sc.nextInt();
		arr = new int[inputCount];
		num1 = new int[inputCount];
		num2 = new int[inputCount];
		for(int i=0; i <inputCount;i++) {
			num1[i] = sc.nextInt();
			num2[i] = sc.nextInt();
			arr[i] = num1[i]+num2[i];
		}
		
		for(int i=0; i<inputCount; i++) {
			System.out.println("Case #"+(i+1)+": "+num1[i]+" + "+num2[i]+" = "+arr[i]);
		}
		
	}
}
