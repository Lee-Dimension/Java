
public class Loop2 {

	public static void main(String[] args) {
		
		for(int num1=1; num1<=9;num1++) {
			for(int num2=2; num2<=9; num2++) {
				if(num2 == 9) {
					System.out.println(num2 +"X"+ num1 + "=" + num1 * num2);
				} else {
				System.out.printf(num2 +"X"+ num1 + "=" + num1 * num2 + "\t");
				}
			}
		}

	}

}
